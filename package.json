{
  "name": "simple-cache",
  "description": "Simple in-memory cache with additional utilities",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "mocha",
    "test:functional:sleep": "ts-node test/functional/sleep.ts",
    "test:functional:memory": "ts-node test/functional/memory.ts",
    "coverage": "nyc npm run test"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karniol/simple-cache.git"
  },
  "keywords": [
    "memcache",
    "in-memory",
    "cache"
  ],
  "author": "Charlie Karniol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karniol/simple-cache/issues"
  },
  "homepage": "https://github.com/karniol/simple-cache#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "pre-commit": "^1.2.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "ts-sinon": "github:karniol/ts-sinon",
    "typescript": "^3.7.5"
  },
  "dependencies": {},
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "mocha": {
    "package": "./package.json",
    "extension": [
      "ts"
    ],
    "spec": [
      "test/**/*.test.ts"
    ],
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.test.ts"
    ],
    "recursive": true,
    "file": [],
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "chai/register-expect"
    ],
    "ui": "bdd",
    "exit": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.test.ts"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "report-dir": "./coverage",
    "temp-dir": "./.nyc_output"
  }
}
